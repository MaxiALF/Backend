<main>
  <h2 class="mt-5 mb-5 text-center">Log In</h2>
  <form class="w-25 m-auto my-5">
    <label for="email" class="form-label">Email</label>
    <input type="email" class="form-control" id="email" required />
    <div class="my-3">
      <label for="password" class="form-label">Password</label>
      <input type="password" class="form-control" id="password" required />
    </div>
    <button id="login" type="button" class="w-100 btn btn-success mt-3">Log In</button>
    <button id="google" type="button" class="w-100 btn btn-success mt-3" >Log
      with Google</button>
  </form>
</main>

<script src="/scripts/login.js"></script>
<script src="/scripts/third.js"></script> 